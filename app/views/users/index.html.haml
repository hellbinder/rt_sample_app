- provide(:title, "All users")
%h1 All users

= form_tag users_path, method: "get", class: "form-search" do
  = text_field_tag :search, params["search"], class: "input-large"  
  %button.btn.btn-primary{type: "submit"}
    %i.icon-user.icon-white
    Search


= will_paginate @users
%ul.users
  = render @users
= will_paginate @users
